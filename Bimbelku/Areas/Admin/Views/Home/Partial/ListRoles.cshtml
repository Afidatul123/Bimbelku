@model UserDashboard

<div class="col-12">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Data Roles</h4>
        </div>
        <table class="table table-dark mb-0">
            <thead>
                <tr>
                    <th scope="col">
                        @Html.DisplayNameFor(item => item.roles[0].Id)
                    </th>
                    <th scope="col">
                        @Html.DisplayNameFor(item => item.roles[0].Name)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.roles)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelitem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelitem => item.Name)
                    </td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>
